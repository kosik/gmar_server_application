<div id="loginPopupContent">
    <div style="padding-top: 10px;"></div>
    
    <div id="googleLoginRegSmall" class="popupHalfAssets">
        <img class="inline-block" alt="google"
             src="../resources/img-res/ico/thirdparty/Social_Media_Socialmedia_network_share_socialnetwork_network-15-32.png" />
        <h5 class="landingSocialBtnLabelAsset inline-block ">Google</h5>
    </div>

    <div id="facebookLoginRegSmall" class="popupHalfAssets">
        <img class="inline-block" alt="facebook"
             src="../resources/img-res/ico/thirdparty/facebook_circle_color-32.png" />    
        <h5 class="landingSocialBtnLabelAsset inline-block">Facebook</h5>
    </div>
    
    <div style="min-height: 21px;"></div>
    
    <input id="loginUserMail" type="email" placeholder='<spring:message code="email" />' 
           tabindex=1 class="leftFloat inline-block popupHalfAssets" />
    <input id="loginPass" type="password" autofocus tabindex=2 class="leftFloat 
           inline-block popupHalfAssets loginPassAssets" />
    
    <a href="https://multitracking.info/#registration"><spring:message code="register" /></a>
    
    <div style="min-height: 11px;" id="loginPopupBottomStub"></div>
    
    <div id="loginBtn" class="rubric_white1_29 roundBtn1 rightFloat" tabindex=3><spring:message code="login" /></div>
</div>